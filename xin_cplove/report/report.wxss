/*report.wxss*/
page{
  overflow-x: hidden;
}
.pull-left{
  float: left;
}
.baoG{
  font-size: 40rpx;
  font-weight: 600;
  color: #000;
  margin-left: 5%;
}
.shuR{
  width: 90%;
  height: 250rpx;
  background: #f5f5f5;
  padding-top: 3%;
  border: none;
  outline: none;
  font-size: 30rpx;
  margin: 0 auto;
  margin-top: 5%;
}
.tuP{
  font-size: 40rpx;
  color: #000;
  margin-top: 5%;
  margin-left: 5%;
  font-weight: 600;
}
.kuang{
  width: 200rpx;
  height: 200rpx;
  text-align: center;
  line-height: 200rpx;
  font-size: 100rpx;
  color: #b9b8be;
  border: 2rpx dotted #b9b8be;
  border-radius: 10rpx;
  margin-top: 5%;
  margin-left: 5%;
}
.tiJ{
  clear: both;
  width: 80%;
  height: 100rpx;
  background: #614af0;
  font-size: 40rpx;
  color: #FFF;
  text-align: center;
  line-height: 100rpx;
  margin: 0 auto;
  border-radius: 50rpx;
  position: fixed;
  bottom: 30rpx;
  left: 0;
  right:0;
}
/*字数限制*/
.font-count{
  position:absolute;
  right:20rpx;
  bottom:0;
}